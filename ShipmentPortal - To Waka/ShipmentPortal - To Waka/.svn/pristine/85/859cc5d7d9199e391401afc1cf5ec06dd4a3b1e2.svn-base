<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.csfe.edison.business.dao.EdisonDAO">

	<sql id="dbName">e_cargo_20101117.dbo.edisonetlid</sql>
	<sql id="azureDB">[AZURE New Staging DB].[SP-GlobalIdSchemaDB]</sql>
	<sql id="azureMasterDB">[azure sql masterdb].[masterdata-testdb]</sql>
	<sql id="cvsDB">edison_prc_cvs_test</sql>
	<sql id="containerStatusLog">CSHKG_SHIPMENT_EVENT_TEST.dbo.ContainerStatusLog</sql>
	<sql id="containerStatusEventLog">CSHKG_SHIPMENT_EVENT_TEST.dbo.ContainerStatusEventLog</sql>
	<sql id="polist"><foreach item="item" index="index" collection="pocompanyCode" open="(" separator="," close=")">#{item}</foreach></sql>
	<sql id="list"><foreach item="item" index="index" collection="list" open="(" separator="," close=")">#{item}</foreach></sql>
	<!-- Retrieve data for 1 or 2 days - configurable for SP2-14 - SP2-20 -->
	<sql id="lastUpdatedDay">DATEADD(DD, DATEDIFF(DD, 0, #{sysdate}), -2)</sql>
	<sql id="companyCodelist"><foreach item="item" index="index" collection="companyCode" open="(" separator="," close=")">#{item}</foreach></sql>
	<sql id="PRINCIPAL_CODE">'TEIN0006'</sql>
	<sql id="CONSIGNEE_CODE">'TEIN0006'</sql>

	<select id="findOrganizations" resultType="Organizations">
		SELECT 
		       ii.id AS [id],
		       'FreightSystem' AS createdby,
		       CONVERT(VARCHAR(23), GETDATE(), 126) AS createddate,
		       'FreightSystem' AS updatedby,
		       CONVERT(VARCHAR(23), GETDATE(), 126) AS updateddate,
		       CASE WHEN ISNULL(ii.code2,'') = 'CENTRALIZED' THEN RTRIM(cch.companycode) ELSE RTRIM(ISNULL(map.OurCode,'')) + '|' + RTRIM(cch.companycode) END AS code,
		       RTRIM(ISNULL(cch.companyname, '')) AS name,
		       RTRIM(ISNULL(ccd.email, '')) AS contactemail,
		       RTRIM(ISNULL(ccd.contact, '')) AS contactname,
		       RTRIM(ccd.telno) AS contactnumber,
		       ISNULL(CONVERT(NVARCHAR(500), RTRIM(ISNULL(ccd.address1,'')) + '\n' + RTRIM(ISNULL(ccd.address2,'')) + '\n' + RTRIM(ISNULL(ccd.address3,'')) + '\n' + RTRIM(ISNULL(ccd.address4,''))), '') AS [address],
		       0 AS edisoninstanceid,
		       ii.azureid AS edisoncompanycodeid,
		       8 AS locationid,
		       2 AS organizationtype,
		       '' AS parentid,
		       1 AS [status]
		FROM <include refid="dbName"/> ii (NOLOCK) 
		INNER JOIN dbo.companyheader cch (NOLOCK)
			ON ii.codetype = 'ORGANIZATIONS'
			AND ii.sysname = 'MASTERDATA'
			AND ii.dbname = #{db}
			AND ii.code = cch.companycode
		INNER JOIN dbo.companydetail ccd (NOLOCK) 
			ON cch.companycode = ccd.companycode
			AND ccd.seq = 1
			--AND RTRIM(ISNULL(ccd.email, '')) <![CDATA[ <> ]]> ''
			--AND RTRIM(ISNULL(ccd.contact, '')) <![CDATA[ <> ]]> ''
			--AND ISNULL(CONVERT(NVARCHAR(500), RTRIM(ISNULL(ccd.address1,'')) + '\n' + RTRIM(ISNULL(ccd.address2,'')) + '\n' + RTRIM(ISNULL(ccd.address3,'')) + '\n' + RTRIM(ISNULL(ccd.address4,''))), '') <![CDATA[ <> ]]> ''
		LEFT JOIN <include refid="cvsDB"/>.dbo.edicodemap map (nolock)
			ON map.EDIName = 'SHIPMENT_PORTAL'
			AND map.codetype = 'CMPYPREFIX'
			AND map.Status = 'Y'
			AND map.PartnerCode = #{db}
		LEFT JOIN <include refid="cvsDB"/>.dbo.edicodemap popmap (nolock)
			ON popmap.EDIName = 'SHIPMENT_PORTAL'
			AND popmap.codetype = 'POPARTYPREFIX'
			AND popmap.Status = 'Y'
			AND popmap.PartnerCode = #{db}
		WHERE ISNULL(popmap.Status, '') <![CDATA[ <> ]]> 'Y'
		AND NOT EXISTS (
			SELECT 1 FROM <include refid="dbName"/> olog (NOLOCK)
				WHERE olog.codetype = 'ORGANIZATIONS'
				AND ii.sysname = 'MASTERDATA'
				AND olog.IsETLtoAzure = 'Y'
				AND ii.code = olog.code
				AND ISNULL(olog.code2,'') = 'CENTRALIZED'
				AND ISNULL(ii.code2,'') = 'CENTRALIZED'
				AND ISNULL(ii.IsETLtoAzure,'') <![CDATA[ <> ]]> 'Y'
				AND ii.id <![CDATA[ <> ]]> olog.id
		)
		AND ii.id in <include refid="list"/>
		UNION ALL
		SELECT 
		       ii.id AS [id],
		       'FreightSystem' AS createdby,
		       CONVERT(VARCHAR(23), GETDATE(), 126) AS createddate,
		       'FreightSystem' AS updatedby,
		       CONVERT(VARCHAR(23), GETDATE(), 126) AS updateddate,
		       ii.code AS code,
		       ii.code3 AS name,
		       NULL AS contactemail,
		       NULL AS contactname,
		       NULL AS contactnumber,
		       NULL AS [address],
		       0 AS edisoninstanceid,
		       dd.Id AS edisoncompanycodeid,
		       8 AS locationid,
		       4 AS organizationtype,
		       '' AS parentid,
		       1 AS [status]
		FROM <include refid="dbName"/> AS ii(NOLOCK)
		LEFT JOIN <include refid="dbName"/> AS dd(NOLOCK) ON ii.code2 = dd.code
		                                        AND dd.codetype = 'ORGANIZATIONS'
		                                        AND ii.sysname = dd.sysname
		                                        AND ii.dbname = dd.dbname
		LEFT JOIN <include refid="cvsDB"/>.dbo.edicodemap popmap (nolock)
			ON popmap.EDIName = 'SHIPMENT_PORTAL'
			AND popmap.codetype = 'POPARTYPREFIX'
			AND popmap.Status = 'Y'
			AND popmap.PartnerCode = #{db}
		WHERE ISNULL(popmap.Status, '') <![CDATA[ <> ]]> 'Y'
		AND NOT EXISTS (
			SELECT 1 FROM <include refid="dbName"/> olog (NOLOCK)
				WHERE olog.codetype = 'PRINCIPALS'
				AND olog.sysname = 'MASTERDATA'
				AND olog.IsETLtoAzure = 'Y'
				AND ii.code = olog.code
				AND ISNULL(ii.IsETLtoAzure,'') <![CDATA[ <> ]]> 'Y'
				AND ii.id <![CDATA[ <> ]]> olog.id
		)
		AND ii.codetype = 'PRINCIPALS'
		AND olog.sysname = 'MASTERDATA'
		AND ii.dbname = #{db}
		AND ii.id in <include refid="list"/>
		UNION ALL
		--PO Parties
		SELECT DISTINCT
		       ii.id AS [id],
		       'FreightSystem' AS createdby,
		       CONVERT(VARCHAR(23), GETDATE(), 126) AS createddate,
		       'FreightSystem' AS updatedby,
		       CONVERT(VARCHAR(23), GETDATE(), 126) AS updateddate,
		       RTRIM(ISNULL(pop.PartyCode, '')) AS code,
		       RTRIM(ISNULL(pop.PartyName, '')) AS name,
		       RTRIM(ISNULL(pop.Email, '')) AS contactemail,
		       RTRIM(ISNULL(pop.ContactPerson, '')) AS contactname,
		       RTRIM(ISNULL(pop.TelNo, '')) AS contactnumber,
		       ISNULL(CONVERT(NVARCHAR(500), RTRIM(ISNULL(pop.AddressLine1,'')) + '\n' + RTRIM(ISNULL(pop.AddressLine2,'')) + '\n' + RTRIM(ISNULL(pop.AddressLine3,'')) + '\n' + RTRIM(ISNULL(pop.AddressLine4,''))), '') AS [address],
		       0 AS edisoninstanceid,
		       ii.azureid AS edisoncompanycodeid,
		       8 AS locationid,
		       2 AS organizationtype,
		       '' AS parentid,
		       1 AS [status]
		FROM <include refid="dbName"/> ii (NOLOCK) 
		INNER JOIN <include refid="cvsDB"/>.dbo.POParty pop (NOLOCK)
			ON ii.codetype = 'ORGANIZATIONS'
			AND olog.sysname = 'MASTERDATA'
			AND ii.dbname = #{db}
			AND ii.seq = 1
			AND pop.CompanyCode = ii.code3
			AND pop.CompanyType = ii.code2
			AND pop.PartyCode = ii.code
			AND pop.PartyType IN ('V','S','P')
		LEFT JOIN <include refid="cvsDB"/>.dbo.edicodemap popmap (nolock)
			ON popmap.EDIName = 'SHIPMENT_PORTAL'
			AND popmap.codetype = 'POPARTYPREFIX'
			AND popmap.Status = 'Y'
			AND popmap.PartnerCode = #{db}
		WHERE ISNULL(popmap.Status, '') = 'Y'
		AND ii.id in <include refid="list"/>
		UNION ALL
		--PO supplier
		SELECT DISTINCT
		       ii.id AS [id],
		       'FreightSystem' AS createdby,
		       CONVERT(VARCHAR(23), GETDATE(), 126) AS createddate,
		       'FreightSystem' AS updatedby,
		       CONVERT(VARCHAR(23), GETDATE(), 126) AS updateddate,
		       RTRIM(ISNULL(cvpo.SupplierCode, '')) AS code,
		       RTRIM(ISNULL(cvpo.SupplierName, '')) AS name,
		       NULL AS contactemail,
		       NULL AS contactname,
		       NULL AS contactnumber,
		       NULL AS [address],
			   0 AS edisoninstanceid,
		       ii.azureid AS edisoncompanycodeid,
		       8 AS locationid,
		       2 AS organizationtype,
		       '' AS parentid,
		       1 AS [status]
		FROM <include refid="dbName"/> ii (NOLOCK) 
		INNER JOIN <include refid="cvsDB"/>.dbo.CompanyValidPO cvpo (NOLOCK)
			ON ii.codetype = 'ORGANIZATIONS'
			AND olog.sysname = 'MASTERDATA'
			AND ii.dbname = #{db}
			AND ii.seq = 2
			AND cvpo.SupplierCode = ii.code
		LEFT JOIN <include refid="cvsDB"/>.dbo.edicodemap popmap (nolock)
			ON popmap.EDIName = 'SHIPMENT_PORTAL'
			AND popmap.codetype = 'POPARTYPREFIX'
			AND popmap.Status = 'Y'
			AND popmap.PartnerCode = #{db}
		WHERE ISNULL(popmap.Status, '') = 'Y'
		AND ii.id in <include refid="list"/>
	</select>
	
	<select id="findShipments" resultType="Shipments">
		SELECT --DISTINCT
		       ii.id AS [id],
		       'FreightSystem' AS createdby,
		       CONVERT(VARCHAR(23), GETDATE(), 126) AS createddate,
		       'FreightSystem' AS updatedby,
		       CONVERT(VARCHAR(23), GETDATE(), 126) AS updateddate,
		       CASE WHEN soh.so_seq > 1 THEN RTRIM(soh.sono) + '-' + CONVERT(VARCHAR(10), soh.so_seq) ELSE RTRIM(soh.sono) END AS shipmentno,
			   RTRIM(soh.ConsigneeCode) AS BuyerCode,
		       SUBSTRING(
		       (
		         SELECT ',' + RTRIM(soix.po_no) AS [text()]
		         FROM dbo.so_item AS soix(NOLOCK)
		         WHERE soh.sono = soix.sono
		               AND soh.so_seq = soix.so_seq
		         GROUP BY soix.po_no
		         FOR XML PATH('')
		       ), 2, 1000) AS [customerreferenceno],
		       RTRIM(ISNULL(soh.agentreference, '')) AS AgentReferenceNo,
			   RTRIM(ISNULL(soh.shipperrefno, '')) AS ShipperReferenceNo,
		       RTRIM(br.cfm_bookingno) AS BookingReferenceNo,
		       CASE WHEN ii.sysname IN ('CRUISE') THEN 'CRUISE' ELSE 'FREIGHT' END AS OrderType,
			   RTRIM(soh.CarrierContactNO) AS CarrierContractNO,
		       CASE WHEN vv.vesselname = 'BY AIR' THEN 'Air'
				WHEN vv.vesselname IN ('BY RAIL', 'BY TRAIN') THEN 'Railway'
				WHEN vv.vesselname IN ('BY TRUCK', 'BY COURIER', 'COURIER') THEN 'Road'
				ELSE 'Sea'
			   END AS modeoftransport,
		       ISNULL(soh.cargoreadydate, '') AS cargoreadydate,
		       soh.bookingdate AS bookingdate,
		       '' AS shipmenttype,
		       CASE WHEN ISNULL(porcity.cityname,'') <![CDATA[ <> ]]> '' THEN RTRIM(UPPER(porcity.cityname)) ELSE RTRIM(ISNULL(fflcity.cityname, '')) END AS shipfrom,
		       ISNULL(ff.etd_date, '') AS shipfrometddate,
		       CASE WHEN ISNULL(podcity.cityname,'') <![CDATA[ <> ]]> '' THEN RTRIM(UPPER(podcity.cityname))
				WHEN ISNULL(soh.vesvoyno_3rd,'') <![CDATA[ <> ]]> '' THEN RTRIM(ff3dcity.cityname)
				WHEN ISNULL(soh.vesvoyno_leg,'') <![CDATA[ <> ]]> '' THEN RTRIM(ff2dcity.cityname)
				ELSE RTRIM(ffdcity.cityname) END AS shipto,
		       (SELECT MAX(v) FROM (VALUES (soh.eta_finaldate), (ff.eta_datedport), (ff2.eta_datedport), (ff3.eta_datedport)) AS value(v)) AS shiptoetadate,
		       RTRIM(soh.movement) AS movement,
		       ISNULL(CONVERT(DECIMAL(18, 4), soii.qty), 0) AS totalpackage,
		       RTRIM(soii.unitcode) AS totalpackageuom,
		       ISNULL(CONVERT(DECIMAL(18, 4), soii.piece), 0) AS totalunit,
		       'Each' AS totalunituom,
		       ISNULL(CONVERT(DECIMAL(18, 4), soii.kgs), 0) AS totalgrossweight,
		       'KGS' AS totalgrossweightuom,
		       ISNULL(CONVERT(DECIMAL(18, 4), soii.net_kgs), 0) AS totalnetweight,
		       'KGS' AS totalnetweightuom,
		       ISNULL(CONVERT(DECIMAL(18, 4), soii.cbm), 0) AS totalvolume,
		       'CBM' AS totalvolumeuom,
		       CASE ISNULL(soh.servicetype, '')
		         WHEN 'PP' THEN 'Port-to-Port'
		         WHEN 'PD' THEN 'Port-to-Door'
		         WHEN 'DD' THEN 'Door-to-Door'
		         WHEN 'DP' THEN 'Door-to-Port'
		         ELSE RTRIM(ISNULL(soh.servicetype, ''))
		       END AS [servicetype],
		       RTRIM(soh.incotermscode) AS Incoterm,
		       CASE soh.so_status 
			   WHEN 'A' THEN 'Active'
			   WHEN 'C' THEN 'Inactive'
			   WHEN 'P' THEN 'Active'
			   END AS status,
		       CASE WHEN soh.movement LIKE 'CY/%' THEN 1 ELSE 0 END AS isfcl
		FROM <include refid="dbName"/> ii (NOLOCK)
			INNER JOIN dbo.so_header soh (NOLOCK) 
				ON ii.dbname = #{db}
				AND ii.sysname = #{sysname}
				AND ii.codetype = 'SHIPMENTS'
				AND ii.code = soh.sono
		        AND ii.seq = soh.so_seq
			LEFT JOIN dbo.vesvoyforwarder ff (NOLOCK) 
				ON soh.vesvoyno = ff.vesvoyno
			LEFT JOIN dbo.vessel vv (NOLOCK)
				ON ff.vesselcode = vv.vesselcode
			LEFT JOIN dbo.city ffdcity (NOLOCK)
				ON ff.dportcode = ffdcity.citycode
			LEFT JOIN dbo.city fflcity (NOLOCK)
				ON ff.lportcode = fflcity.citycode
			LEFT JOIN dbo.country ffdcty (NOLOCK)
				ON ffdcity.countrycode = ffdcty.countrycode
			LEFT JOIN dbo.vesvoyforwarder ff2 (NOLOCK) 
				ON soh.vesvoyno_leg = ff2.vesvoyno
			LEFT JOIN dbo.city ff2dcity (NOLOCK)
				ON ff2.dportcode = ff2dcity.citycode
			LEFT JOIN dbo.country ff2dcty (NOLOCK)
				ON ff2dcity.countrycode = ff2dcty.countrycode
			LEFT JOIN dbo.vesvoyforwarder ff3 (NOLOCK) 
				ON soh.vesvoyno_3rd = ff3.vesvoyno
			LEFT JOIN dbo.city ff3dcity (NOLOCK)
				ON ff3.dportcode = ff3dcity.citycode
			LEFT JOIN dbo.country ff3dcty (NOLOCK)
				ON ff3dcity.countrycode = ff3dcty.countrycode
			OUTER APPLY
			(
				SELECT TOP 1 tcity.cityname, tcty.countryname
				FROM dbo.city AS tcity(NOLOCK)
				INNER JOIN dbo.country AS tcty(NOLOCK) ON tcity.countrycode = tcty.countrycode AND tcity.suspended = 'A' AND tcty.suspended = 'A'
				WHERE (soh.por_name LIKE RTRIM(tcity.cityname) + '%' + RTRIM(tcty.countryname)
					OR soh.por_name = RTRIM(tcity.cityname))
			) AS porcity
			OUTER APPLY
			(
				SELECT TOP 1 tcity.cityname, tcty.countryname
				FROM dbo.city AS tcity(NOLOCK)
				INNER JOIN dbo.country AS tcty(NOLOCK) ON tcity.countrycode = tcty.countrycode AND tcity.suspended = 'A' AND tcty.suspended = 'A'
				WHERE (soh.pod_name LIKE RTRIM(tcity.cityname) + '%' + RTRIM(tcty.countryname)
					OR soh.pod_name = RTRIM(tcity.cityname))
			) AS podcity
			LEFT JOIN
		     (
		       SELECT soi.sono,
		              soi.so_seq,
		              SUM(soi.qty) AS qty,
		              MAX(ISNULL(unitmap.PartnerCode, ISNULL(uu.unitname, soi.unitcode))) AS unitcode,
		              SUM(soi.piece) AS piece,
		              SUM(soi.kgs) AS kgs,
		              SUM(soi.net_kgs) AS net_kgs,
		              SUM(soi.cbm) AS cbm
		       FROM dbo.so_item AS soi(NOLOCK)
		       LEFT JOIN dbo.unit AS uu(NOLOCK) ON ISNULL(uu.unitcode, '') = ISNULL(soi.unitcode, '')
			   LEFT JOIN <include refid="cvsDB"/>.dbo.edicodemap unitmap(NOLOCK)
				ON unitmap.EDIName = 'SHIPMENT_PORTAL'
				AND unitmap.codetype = 'PACKAGEUOM'
				AND unitmap.Status = 'Y'
				AND soi.unitcode = unitmap.OurCode
		       GROUP BY soi.sono,
		                soi.so_seq
		     ) AS soii ON soh.sono = soii.sono
		                  AND soh.so_seq = soii.so_seq
		     LEFT JOIN (
				SELECT e.dbname AS dbname, e.sysname AS sysname, e.code AS cfm_sono, e.seq AS cfm_so_seq, MAX(e.code3) AS cfm_bookingno
				FROM <include refid="dbName"/> AS e(NOLOCK)
				WHERE e.codetype = 'POFULFILLMENT'
					AND ISNULL(e.isetltoazure, '') = 'Y'
				GROUP BY e.dbname, e.sysname, e.code, e.seq
			) br ON ii.dbname = br.dbname
				AND ii.sysname = br.sysname
				AND (soh.sono = br.cfm_sono and soh.so_seq = br.cfm_so_seq)
			LEFT JOIN <include refid="cvsDB"/>.dbo.edicodemap mapp (NOLOCK)
				ON mapp.EDIName = 'SHIPMENT_PORTAL'
				AND mapp.codetype = 'ETLPARAMETER'
				AND mapp.Status = 'Y'
				AND mapp.OurCode = 'SYSNAME_SHIPMENTS_SHIPPERCODE_ACTSHIPPERCODE'
				AND mapp.PartnerCode = ii.sysname
		WHERE ii.id IN <include refid="list"/>
		AND soh.so_status in ('A','P','C')
		AND ((mapp.Status IS NULL) OR (mapp.Status IS NOT NULL AND soh.shippercode = soh.act_shippercode))
	</select> 
	
	<select id="findBillOfLadings" resultType="BillOfLadings">
		SELECT DISTINCT
		       ii.id AS [id],
		       'FreightSystem' AS createdby,
		       CONVERT(VARCHAR(23), GETDATE(), 126) AS createddate,
		       'FreightSystem' AS updatedby,
		       CONVERT(VARCHAR(23), GETDATE(), 126) AS updateddate,
		       RTRIM(hbl.xbl_no) AS billofladingno,
		       ISNULL(aa.azureid, 0) AS executionagentid,
		       RTRIM(hblt.description) AS billofladingtype,
		       CASE 
				WHEN ISNULL(soh.vesvoyno_3rd,'') <![CDATA[ <> ]]> '' THEN RTRIM(ll3.linername) 
				WHEN ISNULL(soh.vesvoyno_leg,'') <![CDATA[ <> ]]> '' THEN RTRIM(ll2.linername) 
				ELSE RTRIM(ll.linername) END AS maincarrier,
		       CASE 
				WHEN ISNULL(soh.vesvoyno_3rd,'') <![CDATA[ <> ]]> '' THEN RTRIM(vv3.vesselnamelong)
				WHEN ISNULL(soh.vesvoyno_leg,'') <![CDATA[ <> ]]> '' THEN RTRIM(vv2.vesselnamelong)
				ELSE RTRIM(vv.vesselnamelong) END AS mainvessel,
		       ISNULL(soii.kgs, 0) AS totalgrossweight,
		       'KGS' AS totalgrossweightuom,
		       ISNULL(soii.net_kgs, 0) AS totalnetweight,
		       'KGS' AS totalnetweightuom,
		       ISNULL(soii.qty, 0) AS totalpackage,
		       RTRIM(soii.unitcode) AS totalpackageuom,
		       ISNULL(soii.cbm, 0) AS totalvolume,
		       'CBM' AS totalvolumeuom,
		       RTRIM(soh.jobno) AS jobnumber,
		       ISNULL(soh.issuedate, '') AS issuedate,
		       CASE 
				WHEN ISNULL(soh.vesvoyno_3rd,'') <![CDATA[ <> ]]> ''
				THEN CASE WHEN vv3.vesselname = 'BY AIR' THEN 'Air'
					WHEN vv3.vesselname IN ('BY RAIL', 'BY TRAIN') THEN 'Railway'
					WHEN vv3.vesselname IN ('BY TRUCK', 'BY COURIER', 'COURIER') THEN 'Road'
					ELSE 'Sea'
				   END
				WHEN ISNULL(soh.vesvoyno_leg,'') <![CDATA[ <> ]]> ''
				THEN CASE WHEN vv2.vesselname = 'BY AIR' THEN 'Air'
					WHEN vv2.vesselname IN ('BY RAIL', 'BY TRAIN') THEN 'Railway'
					WHEN vv2.vesselname IN ('BY TRUCK', 'BY COURIER', 'COURIER') THEN 'Road'
					ELSE 'Sea'
				   END
				ELSE CASE WHEN vv.vesselname = 'BY AIR' THEN 'Air'
					WHEN vv.vesselname IN ('BY RAIL', 'BY TRAIN') THEN 'Railway'
					WHEN vv.vesselname IN ('BY TRUCK', 'BY COURIER', 'COURIER') THEN 'Road'
					ELSE 'Sea'
				   END
			   END AS modeoftransport,
		       CASE WHEN ISNULL(porcity.cityname,'') <![CDATA[ <> ]]> '' THEN RTRIM(UPPER(porcity.cityname)) ELSE RTRIM(ISNULL(fflcity.cityname, '')) END AS shipfrom,
		       ISNULL(ff.etd_date, '') AS shipfrometddate,
		       CASE WHEN ISNULL(podcity.cityname,'') <![CDATA[ <> ]]> '' THEN RTRIM(UPPER(podcity.cityname))
				WHEN ISNULL(soh.vesvoyno_3rd,'') <![CDATA[ <> ]]> '' THEN RTRIM(ff3dcity.cityname)
				WHEN ISNULL(soh.vesvoyno_leg,'') <![CDATA[ <> ]]> '' THEN RTRIM(ff2dcity.cityname)
				ELSE RTRIM(ffdcity.cityname) END AS shipto,
		       (SELECT MAX(v) FROM (VALUES (soh.eta_finaldate), (ff.eta_datedport), (ff2.eta_datedport), (ff3.eta_datedport)) AS value(v)) AS shiptoetadate,
		       RTRIM(soh.movement) AS movement,
		       RTRIM(soh.incotermscode) AS Incoterm
		FROM <include refid="dbName"/> ii (NOLOCK)
			INNER JOIN dbo.housebl hbl (NOLOCK)
				ON ii.dbname = #{db}
				AND ii.sysname = #{sysname}
				AND ii.codetype = 'BILLOFLADINGS'
				AND ii.code = hbl.xbl_no
			INNER JOIN dbo.so_header soh (NOLOCK) 
				ON hbl.mastersono = soh.sono
				AND hbl.masterso_seq = soh.so_seq
			INNER JOIN <include refid="dbName"/> ss (NOLOCK) 
				ON ii.dbname = ss.dbname
				AND ii.sysname = ss.sysname
				AND ss.codetype = 'SHIPMENTS'
				AND soh.sono = ss.code
				AND soh.so_seq = ss.seq
				AND ISNULL(ss.isetltoazure, '') = 'Y'
			INNER JOIN dbo.vesvoyforwarder ff (NOLOCK) 
				ON soh.vesvoyno = ff.vesvoyno  --via MasterSO housebl_no
			INNER JOIN dbo.city ffdcity (NOLOCK)
				ON ff.dportcode = ffdcity.citycode
			INNER JOIN dbo.country ffdcty (NOLOCK)
				ON ffdcity.countrycode = ffdcty.countrycode
			INNER JOIN dbo.city fflcity (NOLOCK)
				ON ff.lportcode = fflcity.citycode
			INNER JOIN dbo.vessel vv (NOLOCK) 
				ON ff.vesselcode = vv.vesselcode
			INNER JOIN dbo.liner ll (NOLOCK) 
				ON ff.linercode = ll.linercode
			LEFT JOIN dbo.vesvoyforwarder ff2 (NOLOCK) 
				ON soh.vesvoyno_leg = ff2.vesvoyno
			LEFT JOIN dbo.city ff2dcity (NOLOCK)
				ON ff2.dportcode = ff2dcity.citycode
			LEFT JOIN dbo.country ff2dcty (NOLOCK)
				ON ff2dcity.countrycode = ff2dcty.countrycode
			LEFT JOIN dbo.vessel vv2 (NOLOCK) 
				ON ff2.vesselcode = vv2.vesselcode
			LEFT JOIN dbo.liner ll2 (NOLOCK) 
				ON ff2.linercode = ll2.linercode
			LEFT JOIN dbo.vesvoyforwarder ff3 (NOLOCK) 
				ON soh.vesvoyno_3rd = ff3.vesvoyno
			LEFT JOIN dbo.city ff3dcity (NOLOCK)
				ON ff3.dportcode = ff3dcity.citycode
			LEFT JOIN dbo.country ff3dcty (NOLOCK)
				ON ff3dcity.countrycode = ff3dcty.countrycode
			LEFT JOIN dbo.vessel vv3 (NOLOCK) 
				ON ff3.vesselcode = vv3.vesselcode
			LEFT JOIN dbo.liner ll3 (NOLOCK) 
				ON ff3.linercode = ll3.linercode
			LEFT JOIN dbo.doc_prefix hblt (NOLOCK) 
				ON hblt.doc_type = 'BLT'
				AND hblt.prefix = soh.bl_type
			OUTER APPLY
			(
				SELECT TOP 1 tcity.cityname, tcty.countryname
				FROM dbo.city AS tcity(NOLOCK)
				INNER JOIN dbo.country AS tcty(NOLOCK) ON tcity.countrycode = tcty.countrycode AND tcity.suspended = 'A' AND tcty.suspended = 'A'
				WHERE (soh.por_name LIKE RTRIM(tcity.cityname) + '%' + RTRIM(tcty.countryname)
					OR soh.por_name = RTRIM(tcity.cityname))
			) AS porcity
			OUTER APPLY
			(
				SELECT TOP 1 tcity.cityname, tcty.countryname
				FROM dbo.city AS tcity(NOLOCK)
				INNER JOIN dbo.country AS tcty(NOLOCK) ON tcity.countrycode = tcty.countrycode AND tcity.suspended = 'A' AND tcty.suspended = 'A'
				WHERE (soh.pod_name LIKE RTRIM(tcity.cityname) + '%' + RTRIM(tcty.countryname)
					OR soh.pod_name = RTRIM(tcity.cityname))
			) AS podcity
			LEFT JOIN (
				SELECT hbl2.xbl_no,
					SUM(lpd2.qty) AS qty,
					MAX(ISNULL(unitmap.PartnerCode, ISNULL(uu.unitname, lpd2.unitcode))) AS unitcode,
					SUM(lpd2.piece) AS piece,
					SUM(lpd2.kgs) AS kgs,
					SUM(lpd2.net_kgs) AS net_kgs,
					SUM(lpd2.cbm) AS cbm
				FROM dbo.housebl hbl2 (NOLOCK)
				LEFT JOIN dbo.so_header soh2 (NOLOCK)
					ON hbl2.housebl_no = soh2.bl_no
				LEFT JOIN dbo.lp_detail lpd2 (NOLOCK) 
					ON soh2.sono = lpd2.sono
		            AND soh2.so_seq = lpd2.so_seq
				LEFT JOIN dbo.unit uu (NOLOCK) ON ISNULL(uu.unitcode, '') = ISNULL(lpd2.unitcode, '')
				LEFT JOIN <include refid="cvsDB"/>.dbo.edicodemap unitmap(NOLOCK)
					ON unitmap.EDIName = 'SHIPMENT_PORTAL'
					AND unitmap.codetype = 'PACKAGEUOM'
					AND unitmap.Status = 'Y'
					AND lpd2.unitcode = unitmap.OurCode
				GROUP BY hbl2.xbl_no
			) soii ON hbl.xbl_no = soii.xbl_no
			INNER JOIN <include refid="dbName"/> aa (NOLOCK) 
				ON ii.dbname = aa.dbname
				AND aa.sysname = 'MASTERDATA'
		        AND aa.codetype = 'ORGANIZATIONS'
				AND aa.code IN <include refid="companyCodelist"/>
				AND ISNULL(aa.isetltoazure, '') = 'Y'
		WHERE ii.id IN <include refid="list"/>
	</select> 
	<select id="findMasterBills" resultType="MasterBills">
		SELECT DISTINCT
			   ii.id AS [id],
			   'FreightSystem' AS createdby,
			   CONVERT(VARCHAR(23), GETDATE(), 126) AS createddate,
			   'FreightSystem' AS updatedby,
			   CONVERT(VARCHAR(23), GETDATE(), 126) AS updateddate,
			   ISNULL(aa.azureid, 0) AS executionagentid,
			   RTRIM(soh.ocean_bl_no) AS masterbillofladingno,
			   'Ocean bill of lading' AS masterbillofladingtype,
			   CASE
				WHEN ISNULL(soh.vesvoyno_3rd,'') <![CDATA[ <> ]]> ''
				THEN CASE WHEN vv3.vesselname = 'BY AIR' THEN 'Air'
					WHEN vv3.vesselname IN ('BY RAIL', 'BY TRAIN') THEN 'Railway'
					WHEN vv3.vesselname IN ('BY TRUCK', 'BY COURIER', 'COURIER') THEN 'Road'
					ELSE 'Sea'
				   END
				WHEN ISNULL(soh.vesvoyno_leg,'') <![CDATA[ <> ]]> ''
				THEN CASE WHEN vv2.vesselname = 'BY AIR' THEN 'Air'
					WHEN vv2.vesselname IN ('BY RAIL', 'BY TRAIN') THEN 'Railway'
					WHEN vv2.vesselname IN ('BY TRUCK', 'BY COURIER', 'COURIER') THEN 'Road'
					ELSE 'Sea'
				   END
				ELSE CASE WHEN vv.vesselname = 'BY AIR' THEN 'Air'
					WHEN vv.vesselname IN ('BY RAIL', 'BY TRAIN') THEN 'Railway'
					WHEN vv.vesselname IN ('BY TRUCK', 'BY COURIER', 'COURIER') THEN 'Road'
					ELSE 'Sea'
				   END
			   END AS modeoftransport,
			   RTRIM(lph.movement) AS movement,
			   RTRIM(lph.carriercontactno) AS carriercontractno,
			   CASE
				WHEN ISNULL(soh.vesvoyno_3rd,'') <![CDATA[ <> ]]> ''
				THEN RTRIM(ll3.linername)
				WHEN ISNULL(soh.vesvoyno_leg,'') <![CDATA[ <> ]]> ''
				THEN RTRIM(ll2.linername)
				ELSE RTRIM(ll.linername)
			   END AS carriername,
			   CASE
				WHEN ISNULL(soh.vesvoyno_3rd,'') <![CDATA[ <> ]]> ''
				THEN CASE WHEN vv3.vesselname = 'BY AIR' THEN RTRIM(ISNULL(airmap3.PartnerCode, ff3.linercode))
					WHEN vv3.vesselname IN ('BY RAIL', 'BY TRAIN', 'BY TRUCK', 'BY COURIER', 'COURIER') THEN ''
					WHEN (ll3.linercode IN ('ALL','TBC') OR RTRIM(ISNULL(ll3.scac_code,'')) = '') THEN 'TBCC' 
					ELSE RTRIM(ll3.scac_code)
				   END
				WHEN ISNULL(soh.vesvoyno_leg,'') <![CDATA[ <> ]]> ''
				THEN CASE WHEN vv2.vesselname = 'BY AIR' THEN RTRIM(ISNULL(airmap2.PartnerCode, ff2.linercode))
					WHEN vv2.vesselname IN ('BY RAIL', 'BY TRAIN', 'BY TRUCK', 'BY COURIER', 'COURIER') THEN ''
					WHEN (ll2.linercode IN ('ALL','TBC') OR RTRIM(ISNULL(ll2.scac_code,'')) = '') THEN 'TBCC' 
					ELSE RTRIM(ll2.scac_code)
				   END
				ELSE CASE WHEN vv.vesselname = 'BY AIR' THEN RTRIM(ISNULL(airmap.PartnerCode, ff.linercode))
					WHEN vv.vesselname IN ('BY RAIL', 'BY TRAIN', 'BY TRUCK', 'BY COURIER', 'COURIER') THEN ''
					WHEN (ll.linercode IN ('ALL','TBC') OR RTRIM(ISNULL(ll.scac_code,'')) = '') THEN 'TBCC'
					ELSE RTRIM(ll.scac_code)
				   END
			   END AS scac,
			   CASE
				WHEN ISNULL(soh.vesvoyno_3rd,'') <![CDATA[ <> ]]> ''
				THEN CASE WHEN vv3.vesselname = 'BY AIR' THEN RTRIM(ISNULL(airmap3.PartnerCode, ff3.linercode)) ELSE '' END
				WHEN ISNULL(soh.vesvoyno_leg,'') <![CDATA[ <> ]]> ''
				THEN CASE WHEN vv2.vesselname = 'BY AIR' THEN RTRIM(ISNULL(airmap2.PartnerCode, ff2.linercode)) ELSE '' END
				ELSE CASE WHEN vv.vesselname = 'BY AIR' THEN RTRIM(ISNULL(airmap.PartnerCode, ff.linercode)) ELSE '' END
			   END AS airlinecode,
			   CASE
				WHEN ISNULL(soh.vesvoyno_3rd,'') <![CDATA[ <> ]]> ''
				THEN CASE WHEN vv3.vesselname = 'BY AIR' THEN RTRIM(ff3.voyage)
					WHEN vv3.vesselname IN ('BY RAIL', 'BY TRAIN', 'BY TRUCK', 'BY COURIER', 'COURIER') THEN ''
					ELSE RTRIM(LTRIM(RTRIM(ISNULL(vv3.vesselnamelong, ISNULL(vv3.vesselname, '')))
						+ CASE WHEN LEN(ISNULL(vv3.vesselnamelong, ISNULL(vv3.vesselname, ''))) = 0 THEN '' WHEN LEN(ISNULL(ff3.voyage, '')) = 0 THEN '' ELSE '/' END
						+ RTRIM(ISNULL(ff3.voyage, ''))))
				   END
				WHEN ISNULL(soh.vesvoyno_leg,'') <![CDATA[ <> ]]> ''
				THEN CASE WHEN vv2.vesselname = 'BY AIR' THEN RTRIM(ff2.voyage)
					WHEN vv2.vesselname IN ('BY RAIL', 'BY TRAIN', 'BY TRUCK', 'BY COURIER', 'COURIER') THEN ''
					ELSE RTRIM(LTRIM(RTRIM(ISNULL(vv2.vesselnamelong, ISNULL(vv2.vesselname, '')))
						+ CASE WHEN LEN(ISNULL(vv2.vesselnamelong, ISNULL(vv2.vesselname, ''))) = 0 THEN '' WHEN LEN(ISNULL(ff2.voyage, '')) = 0 THEN '' ELSE '/' END
						+ RTRIM(ISNULL(ff2.voyage, ''))))
				   END
				ELSE CASE WHEN vv.vesselname = 'BY AIR' THEN RTRIM(ff.voyage)
					WHEN vv.vesselname IN ('BY RAIL', 'BY TRAIN', 'BY TRUCK', 'BY COURIER', 'COURIER') THEN ''
					ELSE RTRIM(LTRIM(RTRIM(ISNULL(vv.vesselnamelong, ISNULL(vv.vesselname, '')))
						+ CASE WHEN LEN(ISNULL(vv.vesselnamelong, ISNULL(vv.vesselname, ''))) = 0 THEN '' WHEN LEN(ISNULL(ff.voyage, '')) = 0 THEN '' ELSE '/' END
						+ RTRIM(ISNULL(ff.voyage, ''))))
				   END
			   END AS vesselflight,
			   CASE
				WHEN ISNULL(soh.vesvoyno_3rd,'') <![CDATA[ <> ]]> ''
				THEN CASE WHEN vv3.vesselname IN ('BY AIR', 'BY RAIL', 'BY TRAIN', 'BY TRUCK', 'BY COURIER', 'COURIER') THEN ''
					ELSE RTRIM(ISNULL(vv3.vesselnamelong, ISNULL(vv3.vesselname, '')))
				   END
				WHEN ISNULL(soh.vesvoyno_leg,'') <![CDATA[ <> ]]> ''
				THEN CASE WHEN vv2.vesselname IN ('BY AIR', 'BY RAIL', 'BY TRAIN', 'BY TRUCK', 'BY COURIER', 'COURIER') THEN ''
					ELSE RTRIM(ISNULL(vv2.vesselnamelong, ISNULL(vv2.vesselname, '')))
				   END
				ELSE CASE WHEN vv.vesselname IN ('BY AIR', 'BY RAIL', 'BY TRAIN', 'BY TRUCK', 'BY COURIER', 'COURIER') THEN ''
					ELSE RTRIM(ISNULL(vv.vesselnamelong, ISNULL(vv.vesselname, '')))
				   END
			   END AS vessel,
			   CASE
				WHEN ISNULL(soh.vesvoyno_3rd,'') <![CDATA[ <> ]]> ''
				THEN CASE WHEN vv3.vesselname IN ('BY AIR', 'BY RAIL', 'BY TRAIN', 'BY TRUCK', 'BY COURIER', 'COURIER') THEN ''
					ELSE RTRIM(ff3.voyage)
				   END
				WHEN ISNULL(soh.vesvoyno_leg,'') <![CDATA[ <> ]]> ''
				THEN CASE WHEN vv2.vesselname IN ('BY AIR', 'BY RAIL', 'BY TRAIN', 'BY TRUCK', 'BY COURIER', 'COURIER') THEN ''
					ELSE RTRIM(ff2.voyage)
				   END
				ELSE CASE WHEN vv.vesselname IN ('BY AIR', 'BY RAIL', 'BY TRAIN', 'BY TRUCK', 'BY COURIER', 'COURIER') THEN ''
					ELSE RTRIM(ff.voyage)
				   END
			   END AS voyage,
			   CASE
				WHEN ISNULL(soh.vesvoyno_3rd,'') <![CDATA[ <> ]]> ''
				THEN CASE WHEN vv3.vesselname = 'BY AIR' THEN RTRIM(ff3.voyage) ELSE '' END
				WHEN ISNULL(soh.vesvoyno_leg,'') <![CDATA[ <> ]]> ''
				THEN CASE WHEN vv2.vesselname = 'BY AIR' THEN RTRIM(ff2.voyage) ELSE '' END
				ELSE CASE WHEN vv.vesselname = 'BY AIR' THEN RTRIM(ff.voyage) ELSE '' END
			   END AS flightno,
			   RIGHT(RTRIM(ISNULL(porcodemap.PartnerCode, soh.por_code)),2) + LEFT(RTRIM(ISNULL(porcodemap.PartnerCode, soh.por_code)),3) AS placeofreceipt,
			   CASE
				WHEN ISNULL(soh.vesvoyno_3rd,'') <![CDATA[ <> ]]> '' THEN RIGHT(RTRIM(ISNULL(lportcodemap3.PartnerCode, ff3.lportcode)),2) + LEFT(RTRIM(ISNULL(lportcodemap3.PartnerCode, ff3.lportcode)),3)
				WHEN ISNULL(soh.vesvoyno_leg,'') <![CDATA[ <> ]]> '' THEN RIGHT(RTRIM(ISNULL(lportcodemap2.PartnerCode, ff2.lportcode)),2) + LEFT(RTRIM(ISNULL(lportcodemap2.PartnerCode, ff2.lportcode)),3)
				ELSE RIGHT(RTRIM(ISNULL(lportcodemap.PartnerCode, ff.lportcode)),2) + LEFT(RTRIM(ISNULL(lportcodemap.PartnerCode, ff.lportcode)),3)
			   END AS portofloading,
			   CASE
				WHEN ISNULL(soh.vesvoyno_3rd,'') <![CDATA[ <> ]]> '' THEN RIGHT(RTRIM(ISNULL(dportcodemap3.PartnerCode, ff3.dportcode)),2) + LEFT(RTRIM(ISNULL(dportcodemap3.PartnerCode, ff3.dportcode)),3)
				WHEN ISNULL(soh.vesvoyno_leg,'') <![CDATA[ <> ]]> '' THEN RIGHT(RTRIM(ISNULL(dportcodemap2.PartnerCode, ff2.dportcode)),2) + LEFT(RTRIM(ISNULL(dportcodemap2.PartnerCode, ff2.dportcode)),3)
				ELSE RIGHT(RTRIM(ISNULL(dportcodemap.PartnerCode, ff.dportcode)),2) + LEFT(RTRIM(ISNULL(dportcodemap.PartnerCode, ff.dportcode)),3)
			   END AS portofdischarge,
			   RIGHT(RTRIM(ISNULL(podcodemap.PartnerCode, soh.pod_code)),2) + LEFT(RTRIM(ISNULL(podcodemap.PartnerCode, soh.pod_code)),3) AS placeofdelivery,
			   CASE WHEN poicity.cityname IS NOT NULL THEN RTRIM(poicity.cityname) ELSE RTRIM(ISNULL(poicodemap.PartnerCode, soh.poi_code)) END AS placeofissue,
			   ISNULL(soh.issuedate, '') AS issuedate,
			   ISNULL(ff.onboarddate, '') AS OnBoardDate
		FROM <include refid="dbName"/> ii (NOLOCK)
			INNER JOIN dbo.so_header soh (NOLOCK) 
				ON ii.dbname = #{db}
				AND ii.sysname = #{sysname}
				AND ii.codetype = 'MASTERBILLS'
				AND ii.code = soh.ocean_bl_no
			INNER JOIN <include refid="dbName"/> ss (NOLOCK) 
				ON ii.dbname = ss.dbname
				AND ii.sysname = ss.sysname
				AND ss.codetype = 'SHIPMENTS'
				AND soh.sono = ss.code
				AND soh.so_seq = ss.seq
				AND ISNULL(ss.isetltoazure, '') = 'Y'
			INNER JOIN dbo.lp_detail lpd (NOLOCK)
				ON soh.sono = lpd.sono
				AND soh.so_seq = lpd.so_seq
			INNER JOIN dbo.lp_header lph (NOLOCK)
				ON lpd.lp_no = lph.lp_no
			INNER JOIN dbo.vesvoyforwarder ff (NOLOCK) 
				ON soh.vesvoyno = ff.vesvoyno  --via MasterSO housebl_no
			INNER JOIN dbo.city ffdcity (NOLOCK)
				ON ff.dportcode = ffdcity.citycode
			INNER JOIN dbo.country ffdcty (NOLOCK)
				ON ffdcity.countrycode = ffdcty.countrycode
			INNER JOIN dbo.city fflcity (NOLOCK)
				ON ff.lportcode = fflcity.citycode
			INNER JOIN dbo.vessel vv (NOLOCK) 
				ON ff.vesselcode = vv.vesselcode
			INNER JOIN dbo.liner ll (NOLOCK) 
				ON ff.linercode = ll.linercode
			LEFT JOIN dbo.vesvoyforwarder ff2 (NOLOCK) 
				ON soh.vesvoyno_leg = ff2.vesvoyno
			LEFT JOIN dbo.city ff2dcity (NOLOCK)
				ON ff2.dportcode = ff2dcity.citycode
			LEFT JOIN dbo.country ff2dcty (NOLOCK)
				ON ff2dcity.countrycode = ff2dcty.countrycode
			LEFT JOIN dbo.vessel vv2 (NOLOCK) 
				ON ff2.vesselcode = vv2.vesselcode
			LEFT JOIN dbo.liner ll2 (NOLOCK) 
				ON ff2.linercode = ll2.linercode
			LEFT JOIN dbo.vesvoyforwarder ff3 (NOLOCK) 
				ON soh.vesvoyno_3rd = ff3.vesvoyno
			LEFT JOIN dbo.city ff3dcity (NOLOCK)
				ON ff3.dportcode = ff3dcity.citycode
			LEFT JOIN dbo.country ff3dcty (NOLOCK)
				ON ff3dcity.countrycode = ff3dcty.countrycode
			LEFT JOIN dbo.vessel vv3 (NOLOCK) 
				ON ff3.vesselcode = vv3.vesselcode
			LEFT JOIN dbo.liner ll3 (NOLOCK) 
				ON ff3.linercode = ll3.linercode
			LEFT JOIN dbo.city poicity (NOLOCK)
				ON soh.poi_code = poicity.citycode
			LEFT JOIN <include refid="cvsDB"/>.dbo.edicodemap airmap (NOLOCK)
				ON airmap.EDIName = 'SHIPMENT_PORTAL'
				AND airmap.codetype = 'AIRLINE'
				AND airmap.Status = 'Y'
				AND ff.linercode = airmap.OurCode
			LEFT JOIN <include refid="cvsDB"/>.dbo.edicodemap airmap2 (NOLOCK)
				ON airmap2.EDIName = 'SHIPMENT_PORTAL'
				AND airmap2.codetype = 'AIRLINE'
				AND airmap2.Status = 'Y'
				AND ff2.linercode = airmap2.OurCode
			LEFT JOIN <include refid="cvsDB"/>.dbo.edicodemap airmap3 (NOLOCK)
				ON airmap3.EDIName = 'SHIPMENT_PORTAL'
				AND airmap3.codetype = 'AIRLINE'
				AND airmap3.Status = 'Y'
				AND ff3.linercode = airmap3.OurCode
			LEFT JOIN <include refid="cvsDB"/>.dbo.edicodemap lportcodemap (NOLOCK)
				ON lportcodemap.EDIName = 'SHIPMENT_PORTAL'
				AND lportcodemap.codetype = 'PORTCODE'
				AND lportcodemap.Status = 'Y'
				AND ff.lportcode = lportcodemap.OurCode
			LEFT JOIN <include refid="cvsDB"/>.dbo.edicodemap lportcodemap2 (NOLOCK)
				ON lportcodemap2.EDIName = 'SHIPMENT_PORTAL'
				AND lportcodemap2.codetype = 'PORTCODE'
				AND lportcodemap2.Status = 'Y'
				AND ff2.lportcode = lportcodemap2.OurCode
			LEFT JOIN <include refid="cvsDB"/>.dbo.edicodemap lportcodemap3 (NOLOCK)
				ON lportcodemap3.EDIName = 'SHIPMENT_PORTAL'
				AND lportcodemap3.codetype = 'PORTCODE'
				AND lportcodemap3.Status = 'Y'
				AND ff3.lportcode = lportcodemap3.OurCode
			LEFT JOIN <include refid="cvsDB"/>.dbo.edicodemap dportcodemap (NOLOCK)
				ON dportcodemap.EDIName = 'SHIPMENT_PORTAL'
				AND dportcodemap.codetype = 'PORTCODE'
				AND dportcodemap.Status = 'Y'
				AND ff.dportcode = dportcodemap.OurCode
			LEFT JOIN <include refid="cvsDB"/>.dbo.edicodemap dportcodemap2 (NOLOCK)
				ON dportcodemap2.EDIName = 'SHIPMENT_PORTAL'
				AND dportcodemap2.codetype = 'PORTCODE'
				AND dportcodemap2.Status = 'Y'
				AND ff2.dportcode = dportcodemap2.OurCode
			LEFT JOIN <include refid="cvsDB"/>.dbo.edicodemap dportcodemap3 (NOLOCK)
				ON dportcodemap3.EDIName = 'SHIPMENT_PORTAL'
				AND dportcodemap3.codetype = 'PORTCODE'
				AND dportcodemap3.Status = 'Y'
				AND ff3.dportcode = dportcodemap3.OurCode
			LEFT JOIN <include refid="cvsDB"/>.dbo.edicodemap porcodemap (NOLOCK)
				ON porcodemap.EDIName = 'SHIPMENT_PORTAL'
				AND porcodemap.codetype = 'PORTCODE'
				AND porcodemap.Status = 'Y'
				AND soh.por_code = porcodemap.OurCode
			LEFT JOIN <include refid="cvsDB"/>.dbo.edicodemap podcodemap (NOLOCK)
				ON podcodemap.EDIName = 'SHIPMENT_PORTAL'
				AND podcodemap.codetype = 'PORTCODE'
				AND podcodemap.Status = 'Y'
				AND soh.pod_code = podcodemap.OurCode
			LEFT JOIN <include refid="cvsDB"/>.dbo.edicodemap poicodemap (NOLOCK)
				ON poicodemap.EDIName = 'SHIPMENT_PORTAL'
				AND poicodemap.codetype = 'PORTCODE'
				AND poicodemap.Status = 'Y'
				AND soh.poi_code = poicodemap.OurCode
			INNER JOIN <include refid="dbName"/> aa (NOLOCK)
				ON ii.dbname = aa.dbname
				AND aa.sysname = 'MASTERDATA'
				AND aa.codetype = 'ORGANIZATIONS'
				AND aa.code IN <include refid="companyCodelist"/>
				AND ISNULL(aa.isetltoazure, '') = 'Y'
		WHERE ii.id IN <include refid="list"/>
      </select> 
	<select id="findContainers" resultType="Containers">
		SELECT DISTINCT
		       ii.id AS [id],
		       'FreightSystem' AS createdby,
		       CONVERT(VARCHAR(23), GETDATE(), 126) AS createddate,
		       'FreightSystem' AS updatedby,
		       CONVERT(VARCHAR(23), GETDATE(), 126) AS updateddate,
		       RTRIM(lph.containerno) AS containerno,
		       RTRIM(lph.lp_no) AS LoadPlanRefNo,
		       RTRIM(lph.containertype) AS containertype,
		       CASE WHEN ISNULL(porcity.cityname,'') <![CDATA[ <> ]]> '' THEN RTRIM(UPPER(porcity.cityname)) ELSE RTRIM(ISNULL(fflcity.cityname, '')) END AS shipfrom,
		       RTRIM(ISNULL(ff.etd_date, '')) AS shipfrometddate,
		       CASE WHEN ISNULL(podcity.cityname,'') <![CDATA[ <> ]]> '' THEN RTRIM(UPPER(podcity.cityname))
				WHEN ISNULL(soh.vesvoyno_3rd,'') <![CDATA[ <> ]]> '' THEN RTRIM(ff3dcity.cityname)
				WHEN ISNULL(soh.vesvoyno_leg,'') <![CDATA[ <> ]]> '' THEN RTRIM(ff2dcity.cityname)
				ELSE RTRIM(ffdcity.cityname) END AS shipto,
		       CASE 
				WHEN ISNULL(soh.vesvoyno_3rd,'') <![CDATA[ <> ]]> '' THEN ff3.eta_datedport 
				WHEN ISNULL(soh.vesvoyno_leg,'') <![CDATA[ <> ]]> '' THEN ff2.eta_datedport 
				ELSE ff.eta_datedport END AS shiptoetadate,
			   lph.loadingdate AS loadingdate,
		       RTRIM(lph.sealno1) AS sealno,
			   RTRIM(lph.sealno2) AS sealno2,
			   RTRIM(lph.carrier_sono) AS carriersono,
		       RTRIM(soh.movement) AS movement,
		       ISNULL(soii.kgs, 0) AS totalgrossweight,
		       'KGS' AS totalgrossweightuom,
		       ISNULL(soii.net_kgs, 0) AS totalnetweight,
		       'KGS' AS totalnetweightuom,
		       ISNULL(soii.qty, 0) AS totalpackage,
		       'Piece' AS totalpackageuom,
		       ISNULL(soii.cbm, 0) AS totalvolume,
		       'CBM' AS totalvolumeuom,
		       CASE WHEN soh.movement LIKE 'CY/%' THEN 1 ELSE 0 END AS isfcl
		FROM <include refid="dbName"/> ii (NOLOCK)
			INNER JOIN dbo.lp_header lph (NOLOCK) 
				ON ii.dbname = #{db}
				AND ii.sysname = #{sysname}
				AND ii.codetype = 'CONTAINERS'
		        AND ii.code = lph.lp_no
		        AND ii.code2 = lph.containerno
			INNER JOIN dbo.lp_detail lpd (NOLOCK)
				ON lph.lp_no = lpd.lp_no
			INNER JOIN dbo.so_header soh (NOLOCK) 
				ON lpd.sono = soh.sono
				AND lpd.so_seq = soh.so_seq
			INNER JOIN <include refid="dbName"/> ss (NOLOCK)		
				ON ii.dbname = ss.dbname
				AND ii.sysname = ss.sysname
		        AND ss.codetype = 'SHIPMENTS'
				AND soh.sono = ss.code
		        AND soh.so_seq = ss.seq
				AND ISNULL(ss.isetltoazure, '') = 'Y'
			INNER JOIN dbo.vesvoyforwarder ff (NOLOCK) 
				ON soh.vesvoyno = ff.vesvoyno
			INNER JOIN dbo.city ffdcity (NOLOCK)
				ON ff.dportcode = ffdcity.citycode
			INNER JOIN dbo.city fflcity (NOLOCK)
				ON ff.lportcode = fflcity.citycode
			INNER JOIN dbo.country ffdcty (NOLOCK)
				ON ffdcity.countrycode = ffdcty.countrycode
			LEFT JOIN dbo.vesvoyforwarder ff2 (NOLOCK) 
				ON soh.vesvoyno_leg = ff2.vesvoyno
			LEFT JOIN dbo.city ff2dcity (NOLOCK)
				ON ff2.dportcode = ff2dcity.citycode
			LEFT JOIN dbo.country ff2dcty (NOLOCK)
				ON ff2dcity.countrycode = ff2dcty.countrycode
			LEFT JOIN dbo.vesvoyforwarder ff3 (NOLOCK) 
				ON soh.vesvoyno_3rd = ff3.vesvoyno
			LEFT JOIN dbo.city ff3dcity (NOLOCK)
				ON ff3.dportcode = ff3dcity.citycode
			LEFT JOIN dbo.country ff3dcty (NOLOCK)
				ON ff3dcity.countrycode = ff3dcty.countrycode
			OUTER APPLY
			(
				SELECT TOP 1 tcity.cityname, tcty.countryname
				FROM dbo.city AS tcity(NOLOCK)
				INNER JOIN dbo.country AS tcty(NOLOCK) ON tcity.countrycode = tcty.countrycode AND tcity.suspended = 'A' AND tcty.suspended = 'A'
				WHERE (soh.por_name LIKE RTRIM(tcity.cityname) + '%' + RTRIM(tcty.countryname)
					OR soh.por_name = RTRIM(tcity.cityname))
			) AS porcity
			OUTER APPLY
			(
				SELECT TOP 1 tcity.cityname, tcty.countryname
				FROM dbo.city AS tcity(NOLOCK)
				INNER JOIN dbo.country AS tcty(NOLOCK) ON tcity.countrycode = tcty.countrycode AND tcity.suspended = 'A' AND tcty.suspended = 'A'
				WHERE (soh.pod_name LIKE RTRIM(tcity.cityname) + '%' + RTRIM(tcty.countryname)
					OR soh.pod_name = RTRIM(tcity.cityname))
			) AS podcity
			INNER JOIN (
				SELECT lph2.lp_no,
					SUM(lpd2.qty) AS qty,
					SUM(lpd2.piece) AS piece,
					SUM(lpd2.kgs) AS kgs,
					SUM(lpd2.net_kgs) AS net_kgs,
					SUM(lpd2.cbm) AS cbm
				FROM dbo.lp_header lph2 (NOLOCK)
					INNER JOIN dbo.lp_detail lpd2 (NOLOCK) 
						ON lph2.lp_no = lpd2.lp_no
		       GROUP BY lph2.lp_no
		     ) soii ON lph.lp_no = soii.lp_no
		WHERE ii.id IN <include refid="list"/>
	</select> 
	<select id="findConsolidations" resultType="Consolidations">
		SELECT DISTINCT
		       ii.id AS [id],
		       'FreightSystem' AS createdby,
		       CONVERT(VARCHAR(23), GETDATE(), 126) AS createddate,
		       'FreightSystem' AS updatedby,
		       CONVERT(VARCHAR(23), GETDATE(), 126) AS updateddate,
		       RTRIM(lph.containerno) AS containerno,
		       RTRIM(lph.sealno1) AS sealno,
		       RTRIM(lph.sealno2) AS sealno2,
		       nn.azureid AS containerid,
		       RTRIM(lph.lp_no) AS ConsolidationNo,
		       CASE WHEN vv.vesselname = 'BY AIR' THEN 'Air'
				WHEN vv.vesselname IN ('BY RAIL', 'BY TRAIN') THEN 'Railway'
				WHEN vv.vesselname IN ('BY TRUCK', 'BY COURIER', 'COURIER') THEN 'Road'
				ELSE 'Sea'
			   END AS modeoftransport,
		       RTRIM(lph.containertype) AS equipmenttype,
		       RTRIM(fflcity.cityname) AS origincfs,
		       ISNULL(ff.cfs_closingdate, '') AS cfscutoffdate,
			   lph.loadingdate AS loadingdate,
			   RTRIM(lph.carrier_sono) AS carriersono,
		       ISNULL(soii.kgs, 0) AS totalgrossweight,
		       'KGS' AS totalgrossweightuom,
		       ISNULL(soii.net_kgs, 0) AS totalnetweight,
		       'KGS' AS totalnetweightuom,
		       ISNULL(soii.qty, 0) AS totalpackage,
		       'Piece' AS totalpackageuom,
		       ISNULL(soii.cbm, 0) AS totalvolume,
		       'CBM' AS totalvolumeuom
		FROM <include refid="dbName"/> ii (NOLOCK) 
			INNER JOIN dbo.lp_header lph (NOLOCK)
				ON ii.dbname = #{db}
				AND ii.sysname = #{sysname}
				AND ii.codetype = 'CONSOLIDATIONS'
				AND ii.code = lph.lp_no
				AND ii.code2 = lph.containerno
			INNER JOIN <include refid="dbName"/> nn (NOLOCK) 
				ON ii.dbname = nn.dbname
				AND ii.sysname = nn.sysname
				AND nn.codetype = 'CONTAINERS'
				AND lph.lp_no = nn.code
		        AND lph.containerno = nn.code2
		        AND ISNULL(nn.isetltoazure, '') = 'Y'
			INNER JOIN dbo.lp_detail lpd (NOLOCK)
				ON lph.lp_no = lpd.lp_no
			INNER JOIN dbo.so_header soh (NOLOCK)
				ON lpd.sono = soh.sono
				AND lpd.so_seq = soh.so_seq
			INNER JOIN <include refid="dbName"/> ss (NOLOCK)		
				ON ii.dbname = ss.dbname
				AND ii.sysname = ss.sysname
		        AND ss.codetype = 'SHIPMENTS'
				AND soh.sono = ss.code
		        AND soh.so_seq = ss.seq
				AND ISNULL(ss.isetltoazure, '') = 'Y'	
			INNER JOIN dbo.vesvoyforwarder ff (NOLOCK) 
				ON soh.vesvoyno = ff.vesvoyno
			INNER JOIN dbo.city fflcity (NOLOCK)
				ON ff.lportcode = fflcity.citycode
			LEFT JOIN dbo.vessel vv (NOLOCK)
				ON ff.vesselcode = vv.vesselcode
			LEFT JOIN (
				SELECT lph2.lp_no,
					SUM(lpd2.qty) AS qty,
					SUM(lpd2.piece) AS piece,
					SUM(lpd2.kgs) AS kgs,
					SUM(lpd2.net_kgs) AS net_kgs,
					SUM(lpd2.cbm) AS cbm
				FROM dbo.lp_header lph2 (NOLOCK)
					LEFT JOIN dbo.lp_detail lpd2 (NOLOCK) 
						ON lph2.lp_no = lpd2.lp_no
		       GROUP BY lph2.lp_no
			) soii 
				ON lph.lp_no = soii.lp_no
		WHERE ii.id IN <include refid="list"/>
	</select> 
	<select id="findConsignments" resultType="Consignments">
		SELECT DISTINCT
		       ii.id AS [id],
		       'FreightSystem' AS createdby,
		       CONVERT(VARCHAR(23), GETDATE(), 126) AS createddate,
		       'FreightSystem' AS updatedby,
		       CONVERT(VARCHAR(23), GETDATE(), 126) AS updateddate,
		       ss.azureid AS shipmentid,
		       'Origin Agent' AS consignmenttype,
		       CONVERT(VARCHAR(23), soh.createdate, 126) AS consignmentdate,
		       CONVERT(VARCHAR(20), aa.azureid) AS executionagentid,
		       RTRIM(fflcity.cityname) AS shipfrom,
		       ff.etd_date AS shipfrometddate,
		       CASE 
				WHEN ISNULL(soh.vesvoyno_3rd,'') <![CDATA[ <> ]]> '' THEN RTRIM(ff3dcity.cityname)
				WHEN ISNULL(soh.vesvoyno_leg,'') <![CDATA[ <> ]]> '' THEN RTRIM(ff2dcity.cityname)
				ELSE RTRIM(ffdcity.cityname) END AS shipto,
		       CASE 
				WHEN ISNULL(soh.vesvoyno_3rd,'') <![CDATA[ <> ]]> '' THEN ff3.eta_datedport 
				WHEN ISNULL(soh.vesvoyno_leg,'') <![CDATA[ <> ]]> '' THEN ff2.eta_datedport 
				ELSE ff.eta_datedport END AS shiptoetadate,
		       CASE soh.so_status 
			   WHEN 'A' THEN 'Active'
			   WHEN 'C' THEN 'Inactive'
			   WHEN 'P' THEN 'Active'
			   END AS STATUS,
		       CASE WHEN vv.vesselname = 'BY AIR' THEN 'Air'
				WHEN vv.vesselname IN ('BY RAIL', 'BY TRAIN') THEN 'Railway'
				WHEN vv.vesselname IN ('BY TRUCK', 'BY COURIER', 'COURIER') THEN 'Road'
				ELSE 'Sea'
			   END AS modeoftransport,
		       RTRIM(soh.movement) AS movement,
		       ISNULL(soii.piece, 0) AS unit,
		       'Each' AS unituom,
		       ISNULL(soii.qty, 0) AS package,
		       RTRIM(soii.unitcode) AS packageuom,
		       ISNULL(soii.cbm, 0) AS volume,
		       'CBM' AS volumeuom,
		       ISNULL(soii.kgs, 0) AS grossweight,
		       'KGS' AS grossweightuom,
		       ISNULL(soii.net_kgs, 0) AS netweight,
		       'KGS' AS netweightuom,
		       CASE WHEN hh.azureid IS NULL THEN '' ELSE CONVERT(VARCHAR,hh.azureid) END AS housebillid,
		       CASE WHEN mm.azureid IS NULL THEN '' ELSE CONVERT(VARCHAR,mm.azureid) END AS masterbillid,
		       0 AS triangletradeflag,
		       (CASE soh.memo_bl_flag
		          WHEN 'M' THEN 1
		          ELSE 0
		        END) AS memobolflag,
		       1 AS [sequence]
		FROM <include refid="dbName"/> ii (NOLOCK)
			INNER JOIN dbo.so_header soh (NOLOCK)
				ON ii.dbname = #{db}
				AND ii.sysname = #{sysname}
				AND ii.codetype = 'CONSIGNMENTS'
				AND ii.code = soh.sono
				AND ii.seq = soh.so_seq
			INNER JOIN <include refid="dbName"/> ss (NOLOCK)
				ON ii.dbname = ss.dbname
				AND ii.sysname = ss.sysname
				AND ss.codetype = 'SHIPMENTS'
				AND soh.sono = ss.code
				AND soh.so_seq = ss.seq
				AND ISNULL(ss.isetltoazure, '') = 'Y'
			LEFT JOIN dbo.housebl hbl (NOLOCK)
				ON soh.bl_no = hbl.housebl_no
			LEFT JOIN <include refid="dbName"/> hh (NOLOCK) 
				ON ss.dbname = hh.dbname
				AND ss.sysname = hh.sysname
				AND hh.codetype = 'BILLOFLADINGS'
				AND hbl.xbl_no = hh.code
				AND ISNULL(hh.isetltoazure, '') = 'Y'
			LEFT JOIN <include refid="dbName"/> mm (NOLOCK)
				ON  ss.dbname = mm.dbname
				AND ss.sysname = mm.sysname
		        AND mm.codetype = 'MASTERBILLS'
		        AND soh.ocean_bl_no = mm.code
		        AND ISNULL(mm.isetltoazure, '') = 'Y'
			INNER JOIN dbo.vesvoyforwarder ff (NOLOCK) 
				ON soh.vesvoyno = ff.vesvoyno  --via MasterSO housebl_no
			INNER JOIN dbo.city ffdcity (NOLOCK)
				ON ff.dportcode = ffdcity.citycode
			INNER JOIN dbo.country ffdcty (NOLOCK)
				ON ffdcity.countrycode = ffdcty.countrycode
			INNER JOIN dbo.city fflcity (NOLOCK)
				ON ff.lportcode = fflcity.citycode
			INNER JOIN dbo.vessel vv (NOLOCK) 
				ON ff.vesselcode = vv.vesselcode
			INNER JOIN dbo.liner ll (NOLOCK) 
				ON ff.linercode = ll.linercode
			LEFT JOIN dbo.vesvoyforwarder ff2 (NOLOCK) 
				ON soh.vesvoyno_leg = ff2.vesvoyno
			LEFT JOIN dbo.city ff2dcity (NOLOCK)
				ON ff2.dportcode = ff2dcity.citycode
			LEFT JOIN dbo.country ff2dcty (NOLOCK)
				ON ff2dcity.countrycode = ff2dcty.countrycode
			LEFT JOIN dbo.vessel vv2 (NOLOCK) 
				ON ff2.vesselcode = vv2.vesselcode
			LEFT JOIN dbo.liner ll2 (NOLOCK) 
				ON ff2.linercode = ll2.linercode
			LEFT JOIN dbo.vesvoyforwarder ff3 (NOLOCK) 
				ON soh.vesvoyno_3rd = ff3.vesvoyno
			LEFT JOIN dbo.city ff3dcity (NOLOCK)
				ON ff3.dportcode = ff3dcity.citycode
			LEFT JOIN dbo.country ff3dcty (NOLOCK)
				ON ff3dcity.countrycode = ff3dcty.countrycode
			LEFT JOIN dbo.vessel vv3 (NOLOCK) 
				ON ff3.vesselcode = vv3.vesselcode
			LEFT JOIN dbo.liner ll3 (NOLOCK) 
				ON ff3.linercode = ll3.linercode
			LEFT JOIN (
				SELECT soi.sono,
					soi.so_seq,
					SUM(soi.qty) AS qty,
					MAX(ISNULL(unitmap.PartnerCode, ISNULL(uu.unitname, soi.unitcode))) AS unitcode,
					SUM(soi.piece) AS piece,
					SUM(soi.kgs) AS kgs,
					SUM(soi.net_kgs) AS net_kgs,
					SUM(soi.cbm) AS cbm
				FROM dbo.so_item soi (NOLOCK)
					LEFT JOIN dbo.unit uu (NOLOCK) ON ISNULL(uu.unitcode, '') = ISNULL(soi.unitcode, '')
					LEFT JOIN <include refid="cvsDB"/>.dbo.edicodemap unitmap(NOLOCK)
						ON unitmap.EDIName = 'SHIPMENT_PORTAL'
						AND unitmap.codetype = 'PACKAGEUOM'
						AND unitmap.Status = 'Y'
						AND soi.unitcode = unitmap.OurCode
				GROUP BY soi.sono, soi.so_seq
			) soii
				ON soh.sono = soii.sono
		        AND soh.so_seq = soii.so_seq
			INNER JOIN <include refid="dbName"/> aa (NOLOCK)
				ON ii.dbname = aa.dbname
				AND aa.sysname = 'MASTERDATA'
		        AND aa.codetype = 'ORGANIZATIONS'
		        AND aa.code IN <include refid="companyCodelist"/>
		WHERE ii.id IN <include refid="list"/>
	</select> 
	<select id="findCargoDetails" resultType="CargoDetails">
		SELECT
		       ii.id AS [id],
		       'FreightSystem' AS createdby,
		       CONVERT(VARCHAR(23), GETDATE(), 126) AS createddate,
		       'FreightSystem' AS updatedby,
		       CONVERT(VARCHAR(23), GETDATE(), 126) AS updateddate,
		       ss.azureid AS shipmentid,
		       CASE WHEN ss.sysname IN ('CRUISE') THEN 'CRUISE' ELSE 'FREIGHT' END AS OrderType,
		       cr.itemseq AS [sequence],
		       soi.marks AS shippingmarks,
		       soi.descriptions AS [description],
		       ISNULL(soi.piece, 0) AS unit,
		       'Each' AS unituom,
		       ISNULL(soi.qty, 0) AS package,
		       RTRIM(ISNULL(unitmap.PartnerCode, ISNULL(uu.unitname, soi.unitcode))) AS packageuom,
		       ISNULL(soi.cbm, 0) AS volume,
		       'CBM' AS volumeuom,
		       ISNULL(soi.kgs, 0) AS grossweight,
		       'KGS' AS grossweightuom,
		       ISNULL(soi.net_kgs, 0) AS netweight,
		       'KGS' AS netweightuom,
		       RTRIM(soi.commoditycode1) AS commodity,
		       RTRIM(soi.htscode) AS hscode,
		       RTRIM(ISNULL(soi.po_no,'')) + '~' + RTRIM(ISNULL(soi.ref_no,'')) + '~' + RTRIM(ISNULL(soi.POSeq,'')) AS productnumber,
		       RTRIM(fflcty.countryname) AS countryoforigin
		FROM <include refid="dbName"/> ii(NOLOCK)
			INNER JOIN <include refid="dbName"/> ss (NOLOCK)
				ON ii.dbname = #{db}
				AND ii.sysname = #{sysname}
				AND ii.codetype = 'CARGODETAILS'
				AND ii.dbname = ss.dbname
				AND ii.sysname = ss.sysname
				AND ss.codetype = 'SHIPMENTS'
				AND ii.code = ss.code
				AND ii.seq = ss.seq
				AND ISNULL(ss.isetltoazure, '') = 'Y'
			INNER JOIN dbo.so_header soh(NOLOCK)
				ON CAST(ss.code AS CHAR) = soh.sono  --ss.code = soh.sono
		        AND ss.seq = soh.so_seq
			INNER JOIN dbo.vesvoyforwarder ff (NOLOCK) 
				ON soh.vesvoyno = ff.vesvoyno
			INNER JOIN dbo.city fflcity (NOLOCK)
				ON ff.lportcode = fflcity.citycode
			INNER JOIN dbo.country fflcty (NOLOCK)
				ON fflcity.countrycode = fflcty.countrycode
			INNER JOIN dbo.so_item soi (NOLOCK)
				ON soi.sono = soh.sono
				AND soi.so_seq = soh.so_seq
				AND soi.itemseq = CONVERT(INT, ISNULL(SUBSTRING(ii.code3,1,CHARINDEX('~',ii.code3,1) - 1),0))
				AND ii.code3 = CONVERT(VARCHAR,ISNULL(soi.itemseq,'')) + '~' + RTRIM(ISNULL(soi.po_no,'')) + '~' + RTRIM(ISNULL(soi.ref_no,''))
			INNER JOIN (
				SELECT tcr.sono, tcr.so_seq, tcr.itemseq, tcr.po_no, tcr.ref_no,
					SUM(tcr.inpcs) AS piece, SUM(tcr.inqty) AS qty, MIN(tcr.inunit) AS unitcode, SUM(tcr.incbm) AS cbm, SUM(tcr.inkgs) AS kgs, SUM(tcr.net_kgs) AS net_kgs
				FROM dbo.cargoreceive tcr (nolock)
				GROUP BY tcr.sono, tcr.so_seq, tcr.itemseq, tcr.po_no, tcr.ref_no
			) cr ON soi.sono = cr.sono
				AND soi.so_seq = cr.so_seq
				AND soi.itemseq = cr.itemseq
				AND ISNULL(soi.po_no,'') = ISNULL(cr.po_no,'')
				AND ISNULL(soi.ref_no,'') = ISNULL(cr.ref_no,'')
				AND ii.code3 = CONVERT(VARCHAR,ISNULL(cr.itemseq,'')) + '~' + RTRIM(ISNULL(cr.po_no,'')) + '~' + RTRIM(ISNULL(cr.ref_no,''))
			LEFT JOIN dbo.unit uu (NOLOCK)
				ON ISNULL(uu.unitcode, '') = ISNULL(soi.unitcode, '')
			LEFT JOIN <include refid="cvsDB"/>.dbo.edicodemap unitmap (NOLOCK)
				ON unitmap.EDIName = 'SHIPMENT_PORTAL'
				AND unitmap.codetype = 'PACKAGEUOM'
				AND unitmap.Status = 'Y'
				AND soi.unitcode = unitmap.OurCode
		WHERE ii.id IN <include refid="list"/>
	</select> 
	<select id="findShipmentLoads" resultType="ShipmentLoads">
		SELECT DISTINCT
		    ii.id AS [id],
		    'FreightSystem' AS createdby,
		    CONVERT(VARCHAR(23), GETDATE(), 126) AS createddate,
		    'FreightSystem' AS updatedby,
		    CONVERT(VARCHAR(23), GETDATE(), 126) AS updateddate,
		    ss.azureid AS shipmentid,
		    cc.azureid AS consignmentid,
		    oo.azureid AS consolidationid,
		    nn.azureid AS containerid,
		    CASE WHEN vv.vesselname = 'BY AIR' THEN 'Air'
				WHEN vv.vesselname IN ('BY RAIL', 'BY TRAIN') THEN 'Railway'
				WHEN vv.vesselname IN ('BY TRUCK', 'BY COURIER', 'COURIER') THEN 'Road'
				ELSE 'Sea'
			END AS modeoftransport,
		    RTRIM(lph.carrier_sono) AS carrierbookingno,
		    CASE WHEN soh.movement LIKE 'CY/%' THEN 1 ELSE 0 END AS isfcl,
		    RTRIM(fflcity.cityname) AS loadingplace,
		    aa.azureid AS loadingpartyid,
		    RTRIM(lph.containertype) AS equipmenttype
		FROM <include refid="dbName"/> ii (NOLOCK)
			INNER JOIN dbo.so_header soh (NOLOCK)
				ON ii.dbname = #{db}
				AND ii.sysname = #{sysname}
				AND ii.codetype = 'SHIPMENTLOADS'
				AND soh.sono = ii.code
				AND soh.so_seq = ii.seq
			INNER JOIN dbo.lp_header lph (NOLOCK)
				ON ii.code2 = lph.lp_no
			INNER JOIN <include refid="dbName"/> ss (NOLOCK)
				ON ii.dbname = ss.dbname
				AND ii.sysname = ss.sysname
				AND ss.codetype = 'SHIPMENTS'
				AND soh.sono = ss.code
				AND soh.so_seq = ss.seq
				AND ISNULL(ss.isetltoazure, '') = 'Y'
			INNER JOIN <include refid="dbName"/> cc (NOLOCK)
				ON ss.dbname = cc.dbname
				AND ss.sysname = cc.sysname
				AND cc.codetype = 'CONSIGNMENTS'
				AND soh.sono = cc.code
				AND soh.so_seq = cc.seq
				AND ISNULL(cc.isetltoazure, '') = 'Y'
			LEFT JOIN <include refid="dbName"/> oo (NOLOCK)
				ON ss.dbname = oo.dbname
				AND ss.sysname = oo.sysname
				AND oo.codetype = 'CONSOLIDATIONS'
				AND lph.lp_no = oo.code
				AND lph.containerno = oo.code2
				AND ISNULL(oo.isetltoazure, '') = 'Y'
			INNER JOIN <include refid="dbName"/> nn (NOLOCK)
				ON ss.dbname = nn.dbname
				AND ss.sysname = nn.sysname
				AND nn.codetype = 'CONTAINERS'
				AND lph.lp_no = nn.code
				AND lph.containerno = nn.code2
				AND ISNULL(nn.isetltoazure, '') = 'Y'
			INNER JOIN dbo.vesvoyforwarder ff (NOLOCK) 
				ON soh.vesvoyno = ff.vesvoyno
			INNER JOIN dbo.city fflcity (NOLOCK)
				ON ff.lportcode = fflcity.citycode
			LEFT JOIN dbo.vessel vv (NOLOCK)
				ON ff.vesselcode = vv.vesselcode
			LEFT JOIN <include refid="dbName"/> aa (NOLOCK)
				ON ii.dbname = aa.dbname
				AND aa.sysname = 'MASTERDATA'
				AND aa.codetype = 'ORGANIZATIONS'
				AND soh.agentcode = aa.code
		WHERE ii.id IN <include refid="list"/>
	</select> 
	<select id="findShipmentLoadDetails" resultType="ShipmentLoadDetails">
		SELECT --DISTINCT
		    ii.id AS [id],
		    'FreightSystem' AS createdby,
		    CONVERT(VARCHAR(23), GETDATE(), 126) AS createddate,
		    'FreightSystem' AS updatedby,
		    CONVERT(VARCHAR(23), GETDATE(), 126) AS updateddate,
		    ss.azureid AS shipmentid,
		    cc.azureid AS consignmentid,
		    dd.azureid AS cargodetailid,
		    sl.azureid AS shipmentloadid,
		    nn.azureid AS containerid,
		    oo.azureid AS consolidationid,
		    ISNULL(lpd.loadorder, 1) AS Sequence,
		    ISNULL(lpd.qty, 0) AS package,
		    RTRIM(ISNULL(unitmap.PartnerCode, ISNULL(uu.unitname, lpd.unitcode))) AS packageuom,
		    ISNULL(lpd.piece, 0) AS unit,
		    'Each' AS unituom,
		    ISNULL(lpd.cbm, 0) AS volume,
		    'CBM' AS volumeuom,
		    ISNULL(lpd.kgs, 0) AS grossweight,
		    'KGS' AS grossweightuom,
		    ISNULL(lpd.net_kgs, 0) AS netweight,
		    'KGS' AS netweightuom
		FROM <include refid="dbName"/> ii (NOLOCK)
			INNER JOIN dbo.lp_header lph (NOLOCK)
				ON ii.dbname = #{db}
				AND ii.sysname = #{sysname}
				AND ii.codetype = 'SHIPMENTLOADDETAILS'
				AND CAST(ii.code2 AS CHAR) = lph.lp_no  --ii.code2 = lph.lp_no
			INNER JOIN dbo.lp_detail lpd (NOLOCK)
				ON lph.lp_no = lpd.lp_no
				AND ii.code = lpd.sono
				AND ii.seq = lpd.so_seq
				AND lpd.itemseq = CONVERT(INT, ISNULL(SUBSTRING(ii.code3,1,CHARINDEX('~',ii.code3,1) - 1),0))
				AND lpd.sub_seq = CONVERT(INT, ISNULL(SUBSTRING(ii.code3,CHARINDEX('~',ii.code3,1) + 1,CHARINDEX('~',ii.code3,CHARINDEX('~',ii.code3,1) + 1) - CHARINDEX('~',ii.code3,1) - 1),0))
			INNER JOIN dbo.CargoReceive cr (NOLOCK)
				ON lpd.sono = cr.sono
				AND lpd.so_seq = cr.so_seq
				AND lpd.itemseq = cr.itemseq
				AND lpd.sub_seq = cr.itemsubseq
				AND ii.code3 = CONVERT(VARCHAR,ISNULL(cr.itemseq,'')) + '~' + CONVERT(VARCHAR,ISNULL(cr.itemsubseq,'')) + '~' + RTRIM(ISNULL(cr.po_no,'')) + '~' + RTRIM(ISNULL(cr.ref_no,''))
			INNER JOIN <include refid="dbName"/> ss (NOLOCK)
				ON ii.dbname = ss.dbname
				AND ii.sysname = ss.sysname
				AND ss.codetype = 'SHIPMENTS'
				AND ii.code = ss.code --SO#
				AND ii.seq = ss.seq --SO_Seq
				AND ISNULL(ss.isetltoazure, '') = 'Y'
			INNER JOIN <include refid="dbName"/> cc (NOLOCK)
				ON ss.dbname = cc.dbname
				AND ss.sysname = cc.sysname
				AND cc.codetype = 'CONSIGNMENTS'
				AND ii.code = cc.code --SO#
				AND ii.seq = cc.seq --SO_Seq
				AND ISNULL(cc.isetltoazure, '') = 'Y'
			INNER JOIN <include refid="dbName"/> dd (NOLOCK)
				ON ss.dbname = dd.dbname
				AND ss.sysname = dd.sysname
		        AND dd.codetype = 'CARGODETAILS'
		        AND lpd.sono = dd.code
		        AND lpd.so_seq = dd.seq
		        AND CONVERT(VARCHAR,ISNULL(cr.itemseq,'')) + '~' + RTRIM(ISNULL(cr.po_no,'')) + '~' + RTRIM(ISNULL(cr.ref_no,'')) = ISNULL(dd.code3, '')
		        AND ISNULL(dd.isetltoazure, '') = 'Y'	
		        AND ISNULL(dd.status, '') NOT IN ('DELETE','DELETED')
			INNER JOIN <include refid="dbName"/> sl (NOLOCK)
				ON ss.dbname = sl.dbname
				AND ss.sysname = sl.sysname
				AND sl.codetype = 'SHIPMENTLOADS'
				AND lpd.sono = sl.code
				AND lpd.so_seq = sl.seq
				AND lpd.lp_no = sl.code2
				AND ISNULL(sl.isetltoazure, '') = 'Y'
			INNER JOIN <include refid="dbName"/> nn (NOLOCK)
				ON ss.dbname = nn.dbname
				AND ss.sysname = nn.sysname
				AND nn.codetype = 'CONTAINERS'
				AND lph.lp_no = nn.code
				AND lph.containerno = nn.code2
				AND ISNULL(nn.isetltoazure, '') = 'Y'
			LEFT JOIN <include refid="dbName"/> oo (NOLOCK)
				ON ss.dbname = oo.dbname
				AND ss.sysname = oo.sysname
				AND oo.codetype = 'CONSOLIDATIONS'
				AND lpd.lp_no = oo.code
				AND lph.containerno = oo.code2
				AND ISNULL(oo.isetltoazure, '') = 'Y'
			LEFT JOIN dbo.unit uu (NOLOCK)
				ON ISNULL(uu.unitcode, '') = ISNULL(lpd.unitcode, '')
			LEFT JOIN <include refid="cvsDB"/>.dbo.edicodemap unitmap (NOLOCK)
				ON unitmap.EDIName = 'SHIPMENT_PORTAL'
				AND unitmap.codetype = 'PACKAGEUOM'
				AND unitmap.Status = 'Y'
				AND lpd.unitcode = unitmap.OurCode
		WHERE ii.id IN <include refid="list"/>
	</select> 
