				AND eid.codetype = 'ACTIVITIES'
				AND nn.codetype = 'CONTAINERS'
				AND eid.code = '3001'
				AND eid.code2 = 'CM'
				AND eid.seq = nn.id  --ediSONETDId.CONTAINERS.id
