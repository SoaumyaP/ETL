			INNER JOIN dbo.lp_header lph (NOLOCK)
				ON nn.code = lph.lp_no
				AND nn.code2 = lph.containerno
			INNER JOIN dbo.vesvoyforwarder ff (NOLOCK)
				ON lph.vesvoyno = ff.vesvoyno
			INNER JOIN dbo.city polcity (NOLOCK)
				ON ff.lportcode = polcity.citycode
