			CONVERT(VARCHAR(23), lph.CYDeliveryDate, 126) AS activitydate,
			polcity.CityName AS location,
			'' AS remark,
			NULL AS resolved,
			'' AS resolution,
			CONVERT(VARCHAR(23), GETDATE(), 126) AS resolutiondate
